let { printf } = import("std/c");

fn main = {
    for i in 0..100 {
        fizzbuzz(i)
    }
};

fn fizzbuzz(i) = {
    if i % 3 == 0 && i % 5 == 0 {
        printf("FizzBuzz\n".data)
    } else if i % 3 == 0 {
        printf("Fizz\n".data)
    } else if i % 5 == 0 {
        printf("Buzz\n".data)
    } else {
        printf("%d\n".data, i)
    }
};
